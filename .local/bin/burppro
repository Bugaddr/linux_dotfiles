#!/bin/sh

exec "$JAVA_HOME/bin/java" --add-opens=java.desktop/javax.swing=ALL-UNNAMED --add-opens=java.base/java.lang=ALL-UNNAMED --add-opens=java.base/jdk.internal.org.objectweb.asm=ALL-UNNAMED --add-opens=java.base/jdk.internal.org.objectweb.asm.tree=ALL-UNNAMED --add-opens=java.base/jdk.internal.org.objectweb.asm.Opcodes=ALL-UNNAMED -javaagent:/bin/loader.jar -Dsun.java2d.dpiaware=true -Dsun.java2d.uiScale=1.0 -noverify -jar /usr/share/burpsuite-pro/burpsuite-pro.jar "$@"
